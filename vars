#MYSQL_CONNECTOR_J=8.0.17
MYSQL_CONNECTOR_J=5.1.48

args+=" MYSQL_CONNECTOR_J=${MYSQL_CONNECTOR_J}"

# set default version to compile using external vars file
[ -z "$tag_latest" ] && source $PWD/vars.85
